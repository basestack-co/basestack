{
  "name": "@basestack/forms",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3003",
    "build": "prisma generate && yarn db:migrate:dev && next build",
    "build:deploy": "prisma generate && yarn db:migrate:prod && next build",
    "build:proxy": "prisma generate --data-proxy && yarn db:migrate:proxy && next build",
    "deploy": "yarn build:deploy",
    "start": "next start",
    "lint": "next lint",
    "test:blocked": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:log": "jest --no-coverage >& ./logs/jest.log",
    "update:packages": "yarn upgrade-interactive --latest",
    "db:connect": "pscale connect $db $branch --port 3306",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate:prod": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:migrate:dev": "prisma migrate dev --schema=./prisma/schema.prisma --skip-seed",
    "db:migrate:proxy": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:migrate:create": "prisma migrate dev --name",
    "db:reset": "prisma migrate reset --schema=./prisma/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "trigger:dev": "npx trigger.dev@beta dev",
    "trigger:deploy:prod": "npx trigger.dev@beta deploy --env prod --skip-typecheck"
  },
  "dependencies": {
    "@basestack/design-system": "*",
    "@basestack/emails": "*",
    "@basestack/hooks": "*",
    "@basestack/ui": "*",
    "@basestack/utils": "*",
    "@emotion/is-prop-valid": "^1.3.0",
    "@hookform/resolvers": "^3.9.0",
    "@lemonsqueezy/lemonsqueezy.js": "^3.2.0",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^5.18.0",
    "@prisma/instrumentation": "^5.18.0",
    "@react-email/render": "^0.0.17",
    "@sentry/nextjs": "^7.112.2",
    "@tanstack/react-query": "^4.36.1",
    "@trigger.dev/sdk": "3.0.0-beta.53",
    "@trpc/client": "^10.45.2",
    "@trpc/next": "^10.45.2",
    "@trpc/react": "^9.26.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.12",
    "eslint-config-next": "^14.2.4",
    "formidable": "^3.5.1",
    "material-symbols": "^0.21.3",
    "next": "^14.2.5",
    "next-auth": "^4.24.7",
    "next-translate": "^2.6.2",
    "polished": "^4.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-spring": "^9.7.4",
    "react-use": "^17.5.1",
    "request-ip": "^3.3.0",
    "sonner": "^1.5.0",
    "styled-components": "^6.1.12",
    "styled-system": "^5.1.5",
    "stylis": "^4.3.2",
    "superjson": "^2.2.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@basestack/config": "*",
    "@basestack/tsconfig": "*",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/cors": "^2.8.17",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.2.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/request-ip": "^0.0.41",
    "@types/styled-system": "^5.1.22",
    "@types/uuid": "^10.0.0",
    "eslint": "8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "3.0.7",
    "jest-styled-components": "^7.2.0",
    "lint-staged": "^15.2.8",
    "next-translate-plugin": "^2.6.2",
    "node-mocks-http": "^1.15.1",
    "prisma": "^5.18.0",
    "turbo": "2.0.12",
    "typescript": "^5.5.4"
  }
}