{
  "name": "fe-flags",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:log": "jest --no-coverage >& ./logs/jest.log",
    "update:packages": "yarn upgrade-interactive --latest",
    "db:connect": "pscale connect $db $branch --port 3306",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema.prisma --skip-seed",
    "db:migrate:create": "prisma migrate dev --name",
    "db:reset": "prisma migrate reset --schema=./prisma/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "@formatjs/intl-utils": "^3.8.4",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.12.0",
    "@reduxjs/toolkit": "^1.8.1",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.2",
    "formik": "^2.2.9",
    "isomorphic-unfetch": "^3.1.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "next": "12.1.6",
    "next-auth": "^4.3.4",
    "polished": "^4.2.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-intl": "^6.0.1",
    "react-redux": "^8.0.1",
    "reselect": "^4.1.5",
    "sh-hooks": "*",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5",
    "ui": "*",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^27.5.1",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/node": "^17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-redux": "^7.1.24",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.25",
    "@types/styled-system": "^5.1.15",
    "@types/uuid": "^8.3.4",
    "config": "*",
    "eslint": "8.15.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "2.0.6",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^12.4.1",
    "next-transpile-modules": "9.0.0",
    "node-mocks-http": "^1.11.0",
    "prisma": "^3.12.0",
    "redux-mock-store": "^1.5.4",
    "tsconfig": "*",
    "turbo": "1.2.9",
    "typescript": "^4.6.4"
  }
}
