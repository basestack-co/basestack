{
  "name": "fe-flags",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:log": "jest --no-coverage >& ./logs/jest.log",
    "update:packages": "yarn upgrade-interactive --latest",
    "db:connect": "pscale connect $db $branch --port 3306",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema.prisma --skip-seed",
    "db:reset": "prisma migrate reset --schema=./prisma/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "@formatjs/intl-utils": "^3.8.4",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.11.1",
    "@reduxjs/toolkit": "^1.8.1",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.0",
    "formik": "^2.2.9",
    "isomorphic-unfetch": "^3.1.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "next": "12.1.4",
    "next-auth": "^4.3.1",
    "polished": "^4.2.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-intl": "^5.24.8",
    "react-redux": "^7.2.8",
    "reselect": "^4.1.5",
    "sh-hooks": "*",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5",
    "ui": "*",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^27.4.1",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isempty": "^4.4.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/node": "^17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-redux": "^7.1.23",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.24",
    "@types/styled-system": "^5.1.15",
    "@types/uuid": "^8.3.4",
    "config": "*",
    "eslint": "8.12.0",
    "jest": "^27.5.1",
    "jest-styled-components": "^7.0.8",
    "next-transpile-modules": "9.0.0",
    "node-mocks-http": "^1.11.0",
    "prisma": "^3.11.1",
    "redux-mock-store": "^1.5.4",
    "tsconfig": "*",
    "typescript": "^4.6.3"
  }
}
