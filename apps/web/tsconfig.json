{
  "extends": "tsconfig/nextjs.json",
  "paths": {
    "@/components/*": ["./components/*"],
    "@/styles/*": ["/styles/*"],
    "@/locales/*": ["/locales/*"],
    "@/types/*": ["/types/*"],
    "@/utils/*": ["/utils/*"],
    "@/graphql/*": ["/graphql/*"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
