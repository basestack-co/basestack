{
  "name": "@basestack/feature-flags",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn db:generate && yarn db:migrate:dev && next build",
    "build:deploy": "yarn db:generate && yarn db:migrate:prod && next build",
    "build:proxy": "yarn db:generate --data-proxy && yarn db:migrate:proxy && next build",
    "deploy": "yarn build:deploy",
    "start": "next start",
    "lint": "next lint",
    "test:blocked": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:log": "jest --no-coverage >& ./logs/jest.log",
    "update:packages": "yarn upgrade-interactive --latest",
    "db:connect": "pscale connect $db $branch --port 3306",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate:prod": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:migrate:dev": "prisma migrate dev --schema=./prisma/schema.prisma --skip-seed",
    "db:migrate:proxy": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:migrate:create": "prisma migrate dev --name",
    "db:reset": "prisma migrate reset --schema=./prisma/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next && rm -rf .swc"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@basestack/design-system": "*",
    "@basestack/emails": "*",
    "@basestack/hooks": "*",
    "@basestack/ui": "*",
    "@basestack/utils": "*",
    "@floating-ui/react": "^0.27.3",
    "@hookform/resolvers": "^3.10.0",
    "@monaco-editor/react": "^4.6.0",
    "@prisma/client": "^6.2.1",
    "@tanstack/react-query": "^5.63.0",
    "@trpc/client": "^11.0.0-rc.682",
    "@trpc/next": "^11.0.0-rc.682",
    "@trpc/react": "^9.26.2",
    "@trpc/react-query": "^11.0.0-rc.682",
    "@trpc/server": "^11.0.0-rc.682",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "eslint-config-next": "^15.1.4",
    "material-symbols": "^0.27.2",
    "next": "^15.1.4",
    "next-auth": "5.0.0-beta.25",
    "next-intl": "^3.26.3",
    "polished": "^4.3.1",
    "random-word-slugs": "^0.1.7",
    "react": "^19.0.0",
    "react-calendar": "^5.1.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-json-view": "^1.21.3",
    "react-spring": "^9.7.5",
    "react-use": "^17.6.0",
    "request-ip": "^3.3.0",
    "sonner": "^1.7.1",
    "styled-components": "^6.1.14",
    "styled-system": "^5.1.5",
    "stylis": "^4.3.4",
    "superjson": "^2.2.2",
    "uuid": "^11.0.5",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@basestack/config": "*",
    "@basestack/tsconfig": "*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/react": "19.0.4",
    "@types/react-calendar": "^4.1.0",
    "@types/react-dom": "^19.0.2",
    "@types/request-ip": "^0.0.41",
    "@types/styled-system": "^5.1.23",
    "@types/uuid": "^10.0.0",
    "eslint": "8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "4.0.0-beta1",
    "jest-styled-components": "^7.2.0",
    "lint-staged": "^15.3.0",
    "node-mocks-http": "^1.16.2",
    "prisma": "^6.2.1",
    "turbo": "2.3.3",
    "typescript": "^5.7.3"
  }
}
