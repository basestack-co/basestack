{
  "name": "@basestack/feature-flags",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && yarn db:migrate:dev && next build",
    "build:deploy": "prisma generate && yarn db:migrate:prod && next build",
    "build:proxy": "prisma generate --data-proxy && yarn db:migrate:proxy && next build",
    "deploy": "yarn build:deploy",
    "start": "next start",
    "lint": "next lint",
    "test:blocked": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:log": "jest --no-coverage >& ./logs/jest.log",
    "update:packages": "yarn upgrade-interactive --latest",
    "db:connect": "pscale connect $db $branch --port 3306",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate:prod": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:migrate:dev": "prisma migrate dev --schema=./prisma/schema.prisma --skip-seed",
    "db:migrate:proxy": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:migrate:create": "prisma migrate dev --name",
    "db:reset": "prisma migrate reset --schema=./prisma/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next"
  },
  "dependencies": {
    "@basestack/design-system": "*",
    "@basestack/hooks": "*",
    "@basestack/utils": "*",
    "@emotion/is-prop-valid": "^1.2.1",
    "@floating-ui/react": "^0.25.4",
    "@hookform/resolvers": "^3.3.1",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^5.2.0",
    "@tanstack/react-query": "^4.35.0",
    "@trpc/client": "^10.38.1",
    "@trpc/next": "^10.38.1",
    "@trpc/react": "^9.26.2",
    "@trpc/react-query": "^10.38.1",
    "@trpc/server": "^10.38.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "eslint-config-next": "^13.4.19",
    "isomorphic-unfetch": "^4.0.2",
    "lru-cache": "^10.0.1",
    "material-symbols": "^0.12.0",
    "next": "^13.4.19",
    "next-auth": "^4.23.1",
    "next-translate": "^2.5.3",
    "polished": "^4.2.2",
    "random-word-slugs": "^0.1.7",
    "react": "^18.2.0",
    "react-calendar": "^4.6.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.1",
    "react-json-view": "^1.21.3",
    "react-select": "^5.7.4",
    "react-spring": "^9.7.1",
    "react-use": "^17.4.0",
    "request-ip": "^3.3.0",
    "styled-components": "^6.0.7",
    "styled-system": "^5.1.5",
    "stylis": "^4.0.0",
    "superjson": "^1.13.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@basestack/config": "*",
    "@basestack/tsconfig": "*",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bcryptjs": "^2.4.3",
    "@types/cors": "^2.8.14",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@types/react": "18.2.21",
    "@types/react-calendar": "^4.1.0",
    "@types/react-dom": "^18.2.7",
    "@types/request-ip": "^0.0.38",
    "@types/styled-system": "^5.1.17",
    "@types/uuid": "^9.0.3",
    "eslint": "8.48.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "3.0.5",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^14.0.1",
    "next-translate-plugin": "^2.5.3",
    "next-transpile-modules": "10.0.1",
    "node-mocks-http": "^1.13.0",
    "prisma": "^5.2.0",
    "turbo": "1.10.13",
    "typescript": "^5.2.2"
  }
}
