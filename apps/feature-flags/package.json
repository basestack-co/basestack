{
  "name": "@basestack/feature-flags",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && yarn db:migrate:dev && next build",
    "build:deploy": "prisma generate && yarn db:migrate:prod && next build",
    "build:proxy": "prisma generate --data-proxy && yarn db:migrate:proxy && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:log": "jest --no-coverage >& ./logs/jest.log",
    "update:packages": "yarn upgrade-interactive --latest",
    "db:connect": "pscale connect $db $branch --port 3306",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate:prod": "prisma migrate deploy --schema=./prisma/schema.prisma --skip-seed",
    "db:migrate:dev": "prisma migrate dev --schema=./prisma/schema.prisma --skip-seed",
    "db:migrate:proxy": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy --schema=./prisma/schema.prisma --skip-seed",
    "db:migrate:create": "prisma migrate dev --name",
    "db:reset": "prisma migrate reset --schema=./prisma/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next"
  },
  "dependencies": {
    "@basestack/design-system": "*",
    "@basestack/hooks": "*",
    "@basestack/utils": "*",
    "@floating-ui/react-dom": "^1.3.0",
    "@hookform/resolvers": "^2.9.11",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.10.1",
    "@tanstack/react-query": "^4.24.6",
    "@trpc/client": "^10.11.1",
    "@trpc/next": "^10.11.1",
    "@trpc/react": "^9.26.2",
    "@trpc/react-query": "^10.11.1",
    "@trpc/server": "^10.11.1",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.7",
    "eslint-config-next": "^13.1.6",
    "isomorphic-unfetch": "^4.0.2",
    "material-symbols": "^0.4.4",
    "next": "^13.1.6",
    "next-auth": "^4.19.2",
    "polished": "^4.2.2",
    "random-word-slugs": "^0.1.6",
    "react": "^18.2.0",
    "react-calendar": "^4.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.1",
    "react-json-view": "^1.21.3",
    "react-select": "^5.7.0",
    "react-spring": "^9.6.1",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5",
    "superjson": "^1.12.2",
    "uuid": "^9.0.0",
    "zod": "^3.20.6",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@basestack/config": "*",
    "@basestack/tsconfig": "*",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/react": "18.0.28",
    "@types/react-calendar": "^3.9.0",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.25",
    "@types/styled-system": "^5.1.16",
    "@types/uuid": "^9.0.0",
    "eslint": "8.34.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "3.0.1",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^13.1.2",
    "next-transpile-modules": "10.0.0",
    "node-mocks-http": "^1.12.1",
    "prisma": "^4.10.1",
    "turbo": "1.7.4",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "@types/react": "^18.0.0"
  },
  "engines": {
    "node": ">=16 <17"
  }
}
