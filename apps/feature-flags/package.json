{
  "name": "@basestack/feature-flags",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && yarn db:migrate && next build",
    "build:proxy": "prisma generate --data-proxy && yarn db:migrate:proxy && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:log": "jest --no-coverage >& ./logs/jest.log",
    "update:packages": "yarn upgrade-interactive --latest",
    "db:connect": "pscale connect $db $branch --port 3306",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema.prisma --skip-seed",
    "db:migrate:proxy": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy",
    "db:migrate:create": "prisma migrate dev --name",
    "db:reset": "prisma migrate reset --schema=./prisma/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next"
  },
  "dependencies": {
    "@basestack/design-system": "*",
    "@basestack/hooks": "*",
    "@basestack/utils": "*",
    "@floating-ui/react-dom": "^1.0.0",
    "@hookform/resolvers": "^2.9.10",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.1.1",
    "@trpc/client": "^9.26.2",
    "@trpc/next": "^9.26.2",
    "@trpc/react": "^9.26.2",
    "@trpc/server": "^9.26.2",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.4",
    "eslint-config-next": "^13.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^13.0.0",
    "next-auth": "^4.10.3",
    "polished": "^4.2.2",
    "random-word-slugs": "^0.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.1",
    "react-query": "^3.39.1",
    "react-spring": "^9.5.2",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5",
    "superjson": "^1.9.1",
    "uuid": "^8.3.2",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@basestack/config": "*",
    "@basestack/tsconfig": "*",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "@types/react": "18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@types/styled-system": "^5.1.15",
    "@types/uuid": "^8.3.4",
    "eslint": "8.21.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "2.0.7",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^13.0.3",
    "next-transpile-modules": "9.0.0",
    "node-mocks-http": "^1.11.0",
    "prisma": "^4.1.1",
    "turbo": "1.4.2",
    "typescript": "^4.7.4"
  },
  "overrides": {
    "@types/react": "^18.0.0"
  }
}
